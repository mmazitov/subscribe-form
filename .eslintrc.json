{
	"extends": [
		"next/core-web-vitals",
		"next/typescript",
		"plugin:prettier/recommended",
		"plugin:react/recommended",
		"plugin:react/jsx-runtime"
	],
	"rules": {
		"prettier/prettier": [
			"warn",
			{
				"endOfLine": "auto"
			}
		],
		"react/react-in-jsx-scope": "off",
		"react/jsx-uses-react": "off"
	},
	"settings": {
		"react": {
			"version": "detect"
		}
	}
}
